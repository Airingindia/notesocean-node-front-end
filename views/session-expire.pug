doctype html
html(lang="en")
    head
        meta(charset="UTF-8")
        meta(http-equiv="X-UA-Compatible", content="IE=edge")
        meta(name="viewport", content="width=device-width, initial-scale=1.0")
        title Session Expire
        include includes/bootstrap
        //- include includes/amplitude
        link(rel="manifest", href="/manifest.webmanifest")
    body 
        header
            include includes/navbar
        section.container.mt-5.py-5
            h2.text-center Session has expired
            p.text-center It seems you have changed your password or login on another device
            center 
                a(href="/account/login").btn.btn-primary.text-center  
                    i.fa.fa-key.mx-2
                    span Login Again
                
        script(src="/javascripts/common.js")