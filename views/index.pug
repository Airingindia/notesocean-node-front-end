    section.banner-search.main-section
      .headings.text-center
        h1.animate__animated.animate__fadeInUp.my-4 Find the Perfect 
          span.public Notes
        p.animate__animated.animate__fadeInUp Notesocean is the best destination for students
          br
          span to find perfect notes for every subjects , chapters &amp; courses.
        br
        form.example.animate__animated.animate__fadeInUp(action='/search')
          input(type='text' placeholder='search your notes' name='query',required)
          button(type='submit')
            i.fa.fa-search
        .realted-searches.mt-2.d-none
          a(href="/search?query=engineering").text-danger.mx-3
            small Engineeering Notes
          a(href="/search?query=ncrt").text-danger.mx-3 
            small Management Notes
          a(href="/search?query=class+12th").text-danger.mx-3
            small Class 12th Notes
          a(href="/search?query=cbse").text-danger.mx-3
            small CBSE Notes
          a(href="/search?query=ncrt").text-danger.mx-3 
            small NCRT Notes
    section.categories.categories-courses.wow.animate__animated.animate__fadeIn
      .container.py-5
        h3 Notes By 
          span.color-red Courses
        p Find your notes by your courses 
          a(href="/courses").color-red View all
        .container.categories-item-cont
          .row
            - let count = 0;
            -for(let i = 0;i<12;i++){
              - count++;
              - var item = courses[Math.floor(Math.random()*courses.length)];

                .col-6.col-lg-4.mt-3
                  a(href="/courses/"+url.make(item))
                    .card.shadow.border-0.p-5.text-center.h-100.d-flex.justify-content-center.align-items-center.w-100.h-100
                      h6 #{item}

            -}

    section.categories.categories-subject.wow.animate__animated.animate__fadeIn
      .container.py-5
        h3 Notes By 
