                        span
                          i.fa.fa-globe.mx-1
                          small=views
                        span.mx-1
                          i.fa.fa-file.mx-1 
                          small=pages
                        span.mx-1
                          i.fa.fa-clock.mx-1
                          small=actual_time
        - }    
